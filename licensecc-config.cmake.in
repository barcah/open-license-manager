# - Config file for the LICENSECC package
# It defines the following variables
#  LICENSECC_FOUND - indicates that the module was found
#  LICENSECC_LIBRARIES - libraries
#  LICENSECC_INCLUDE_DIR - include directories
#  LICENSECC_LIBRARIES_DIR - directories for linker

@PACKAGE_INIT@

list(APPEND LICENSECC_LIBRARIES @PACKAGE_INSTALL_LIBRARIES@)

set_and_check(LICENSECC_INCLUDE_DIR "@PACKAGE_INSTALL_INCLUDE_DIR@")
set_and_check(LICENSECC_LIBRARIES_DIR "@PACKAGE_INSTALL_LIBRARIES_DIR@")

set(LICENSECC_FOUND TRUE)

message(STATUS "LICENSECC_FOUND = " ${LICENSECC_FOUND})
message(STATUS "LICENSECC_INCLUDE_DIR = " ${LICENSECC_INCLUDE_DIR})
message(STATUS "LICENSECC_LIBRARIES_DIR = " ${LICENSECC_LIBRARIES_DIR})
message(STATUS "LICENSECC_LIBRARIES = " ${PACKAGE_INSTALL_LIBRARIES})
